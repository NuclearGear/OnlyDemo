	<dubbo:reference id="adHocLoginService" interface="com.pajk.user.admin.AdHocLoginService" version="LATEST" timeout="30000" />
	<dubbo:reference id="operationsService" interface="com.pajk.yachtshowcase.client.OperationsService" version="LATEST" timeout="30000" />
	<dubbo:reference id="reportService" interface="com.pajk.yachtshowcase.client.ReportService" version="LATEST" timeout="30000" />
	<dubbo:reference id="clientService" interface="com.pajk.yachtshowcase.client.ClientService" version="LATEST" timeout="30000" />
	<dubbo:reference id="bIService" interface="com.pajk.polaris.client.BIService" version="LATEST" timeout="30000" />
</beans>